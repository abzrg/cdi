// ************************************************************************ //

FoamFile
{
    version 2.0;
    format  ascii;
    class   dictionary;
    object  funckySetFieldsDict;
}

// ************************************************************************ //

expressions
(
    internalFields
    {
        field       U;
        variables   "Y=pos().y;Ymax=max(pos().y);Ymin=min(pos().y);U_avg=1;n=vector(1,0,0);";
        expression  "n*1.5*U_avg*(1-sqr((Y-Ymin)/(Ymax-Ymin)))";
        // condition   "";
        keepPatches 1; // Keep the boundary condition that set before
    }
);
