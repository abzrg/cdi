// ************************************************************************ //

FoamFile
{
    version 2.0;
    format  ascii;
    class   dictionary;
    object  funckySetFieldsDict;
}

// ************************************************************************ //

expressions
(
    internalFields
    {
        field       U;
        variables   "Y=pos().y;Ymax=1;Ymin=0;U_avg=1;n=vector(1,0,0);";
        expression  "n*2*U_avg*(1-sqr((Y)/(Ymax-Ymin)))";
        condition   "pos().y <= 1";
        keepPatches 1; // Keep the boundary condition that set before
    }
);
// vim: ft=foam
