FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     cdiFoam;

startFrom       startTime;

startTime       0;
// startTime       9245.4;

stopAt          endTime;

endTime         9248; // 0-9248 ; 9248-18496
// endTime         18496; // 0-9248 ; 9248-18496

deltaT          0.0285; // 0.0285;

writeControl    timeStep;

writeInterval   100;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression on;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


