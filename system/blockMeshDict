FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.01;

vertices
(
    // Back
    (0 0 -0.5)
    (5 0 -0.5)
    (0 2 -0.5)
    (5 2 -0.5)
    // Front
    (0 0 0.5)
    (5 0 0.5)
    (0 2 0.5)
    (5 2 0.5)
);

blocks
(
   	hex (0 1 3 2 4 5 7 6) (20 8 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
   	inlet
   	{
        type patch;
      	faces
        (
            (0 4 6 2)
        );
    }
    outlet
    {
        type patch;
        faces
        (
            (1 3 7 5)
        );
    }
    wall
    {
		type wall;
		faces
		(
			(3 2 6 7)
			(0 1 5 4)
		);
	}
	frontAndBack
	{
		type empty;
		faces
		(
			// Back
			(0 2 3 1)
			// Front
			(4 5 7 6)
		);
	}
);

mergePatchPairs
(
);

