FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      c;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 20;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform $internalFiled;
    }

    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            zeroGradient;
    }

    midPlane
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

